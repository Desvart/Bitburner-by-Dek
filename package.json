{
   "name": "bitburner-by-dek",
   "version": "Alpha-1.0.0",
   "description": "Bitburner by Dek",
   "author": "Desvart",
   "type": "module",
   "scripts": {
      "transpile-all": "tsc --build",
      "watch:transpile-all": "tsc --build --watch --preserveWatchOutput",
      "watch:post-transpile-all": "node build/out/post-transpilation-cleanup.js dist/ci",
      "watch:build-all": "concurrently -n \"trans,clean\" -c \"magenta,yellow\" \"npm run watch:local-transpile-all\" \"npm run watch:local-sync-all\"",
      "watch:deploy": "bitburner-filesync",
      "watch:build-and-deploy": "concurrently -n \"trans,clean,deploy\" -c \"magenta,yellow,green\" \"npm run watch:transpile-all\" \"npm run watch:post-transpile-all\" \"npm run watch:deploy\""
   },
   "dependencies": {
      "chokidar": "latest"
   },
   "devDependencies": {
      "typescript": "latest",
      "eslint-config-airbnb-typescript": "latest",
      "eslint-config-airbnb-base": "latest",
      "@typescript-eslint/eslint-plugin": "latest",
      "@typescript-eslint/parser": "latest",
      "eslint": "latest",
      "eslint-plugin-import": "latest",
      "eslint-import-resolver-typescript": "latest",
      "prettier": "latest",
      "eslint-config-prettier": "latest",
      "eslint-plugin-prettier": "latest",
      "concurrently": "latest",
      "fs-extra": "latest",
      "@types/fs-extra": "latest",
      "bitburner-filesync": "latest"
   }
}
